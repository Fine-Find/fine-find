{
  "name": "fine-find",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:cert": "cross-env NODE_OPTIONS='--inspect' node server.js",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write ./src && eslint ./src/**/*.{js,jsx,ts,tsx} --fix",
    "lint": "prettier --check ./src && eslint ./src/**/*.{js,jsx,ts,tsx} && npm run type-check",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "autoprefixer": "^10.2.4",
    "autosuggest-highlight": "^3.1.1",
    "firebase": "^8.7.1",
    "firebase-admin": "^9.9.0",
    "instagram-web-api": "^2.2.2",
    "next": "10.0.6",
    "next-auth": "^3.13.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "^6.14.2",
    "react-infinite-scroll-hook": "^4.0.1",
    "shopify-api-node": "^3.6.13",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@next/eslint-plugin-next": "^10.0.8",
    "@storybook/addon-actions": "^6.3.4",
    "@storybook/addon-essentials": "^6.3.4",
    "@storybook/addon-links": "^6.3.4",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack5": "^6.3.4",
    "@storybook/manager-webpack5": "^6.3.4",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.3.4",
    "@testing-library/react": "^11.2.5",
    "@types/jest": "^26.0.24",
    "@types/node": "^14.14.22",
    "@types/prop-types": "^15.7.4",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.1",
    "identity-obj-proxy": "^3.0.0",
    "import-sort-style-module": "^6.0.0",
    "jest": "^26.6.3",
    "jest-watch-typeahead": "^0.6.1",
    "lint-staged": "^10.5.4",
    "postcss": "^8.2.7",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "prettier-plugin-import-sort": "0.0.7",
    "sass": "^1.32.8",
    "sass-loader": "^12.1.0",
    "typescript": "^4.1.3",
    "webpack": "^5.24.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run type-check"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx|js|jsx)": [
      "npm run lint",
      "npm run format"
    ]
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  }
}
